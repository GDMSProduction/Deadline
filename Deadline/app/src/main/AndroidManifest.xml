            android:name="com.DeadLine.leon.deadline.RolePermissions"
            android:label="@string/title_activity_role_permissions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.DeadLine.leon.deadline.FileManager"
            android:label="@string/title_activity_file_manager"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>



</manifest>